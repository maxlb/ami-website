            </div>
          </div>
          
          <!-- Nationalité, pays de naissance -->
          <div class="row">
            <div class="input-field col s6">
              <i class="material-icons prefix default_color_text">public</i>
              <input id="MAJnationalite" type="text" class="autocomplete nationalite validate" required> 
              <label for="MAJnationalite">Nationalité</label>
              <span class="helper-text" data-error="Veuillez renseigner ce champs" data-success="">Champs obligatoire</span>
            </div>
            <div class="input-field col s6">
              <i class="material-icons prefix default_color_text">public</i>
              <input id="MAJpaysNaissance" type="text" class="autocomplete pays-naissance validate" required">
